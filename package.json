{
  "name": "qa-infobox",
  "version": "0.1.0",
  "description": "A modal box which displays browser / OS info and additionally (build) data from JSON files",
  "author": {
    "name": "pro!vision GmbH",
    "url": "http://www.pro-vision.de"
  },
  "contributors": [
    {
      "name": "Fritz Stelluto",
      "email": "fstelluto@pro-vision.de"
    }
  ],
  "homepage": "https://github.com/pro-vision/qa-infobox",
  "bugs": {
    "url": "https://github.com/pro-vision/qa-infobox/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pro-vision/qa-infobox.git"
  },
  "scripts": {
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint . --ignore-path .gitignore",
    "lint:css": "stylelint **/*.css",
    "lint:md": "remark . --frail --use lint --quiet",
    "pretest": "npm run lint",
    "release": "npmpub",
    "test": "jest",
    "watch:test": "jest --watch",
    "watch:lint:js": "onchange **/*.js -i -- npm run lint:js",
    "start": "webpack-dev-server --display-error-details --open"
  },
  "main": "lib/index.js",
  "keywords": [
    "qa",
    "modal",
    "pro!vision"
  ],
  "jest": {
    "testRegex": "/__tests__/.*(test|spec)\\.jsx?$"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.21.0",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-polyfill": "6.20.0",
    "babel-preset-es2015": "6.18.0",
    "eslint": "3.13.0",
    "eslint-config-pv": "1.0.3",
    "eslint-plugin-import": "2.2.0",
    "jest": "18.1.0",
    "npm-run-all": "4.0.0",
    "npmpub": "3.1.0",
    "onchange": "3.2.1",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "remark-cli": "2.1.0",
    "remark-lint": "5.4.0",
    "remark-preset-lint-recommended": "1.0.0",
    "stylelint": "7.7.1",
    "stylelint-config-pv": "0.2.6",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2"
  },
  "dependencies": {}
}
